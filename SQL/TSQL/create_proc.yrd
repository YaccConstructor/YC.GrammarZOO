[<AllPublic>]
module Create_proc

open MsCommon, Common, Stmnt, Set_stmnt, Select_crud

create_proc:
    'CREATE' ('PROCEDURE' | 'PROC')
    [ident '.'] ident [';' DEC_NUMBER]
    brace_opt<'(' comma_list<proc_formal_param> ')'>
    ['WITH' (execute_as)+]
    ['FOR' 'REPLICATION']
    'AS' (proc_body_stmnt ';'?)*
    ';'?


execute_as:
    ('EXEC' | 'EXECUTE')
    'AS'
    ('CALLER' | 'SELF' | 'OWNER' |ident '=' STRING_CONST) 
