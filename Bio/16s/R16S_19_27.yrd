(*
Variation in secondary structure of the 16S rRNA molecule in cyanobacteria
with implications for phylogenetic analysis
*)


(*include "../common/common.yrd"

module Main

open Common
*)


stem1<s>:
      A s U
    | U s A
    | C s G
    | G s C
    | G s U
    | U s G
    | A s G
    | G s A


stem2<s>: stem1<stem1<s>>
stem4<s>: stem2<stem2<s>>
stem6<s>: stem4<stem2<s>>
stem8<s>: stem4<stem4<s>>

mk_stem<s>: 
      A mk_stem<s> U
    | U mk_stem<s> A
    | C mk_stem<s> G
    | G mk_stem<s> C
    | G mk_stem<s> U
    | U mk_stem<s> G
    | G mk_stem<s> A
    | A mk_stem<s> G
    | s

stem<s>: mk_stem<stem4<s>>
stem2g<s>: mk_stem<stem2<s>>
stem4g<s>: mk_stem<stem4<s>>

gstem<s>: 
      A stem<s> U
    | U stem<s> A
    | C stem<s> G
    | G stem<s> C
    | G stem<s> U
    | U stem<s> G
    | G stem<s> A
    | A stem<s> G
    | G stem<s> G
    | A s U
    | U s A
    | C s G
    | G s C
    | G s U
    | U s G
    | A s G
    | G s A
    | G s G

inline any: A | U | G | C

[<Start>]
full: folded any?

folded: midle_part_root

inline any_1_2: any*[1..2]
inline any_1_3: any*[1..3]
